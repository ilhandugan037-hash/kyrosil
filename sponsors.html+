    <link href="https://fonts.googleapis.com/css2?family=Outfit:wght@300;500;700&display=swap" rel="stylesheet">
    
    <link rel="stylesheet" href="style.css">

    <style>
        /* BU SAYFAYA ÖZEL STİLLER */
        body { margin: 0; padding: 0; background-color: #0a0e17; } /* style.css gelmezse diye yedek */

        .page-header {
            text-align: center;
            padding: 4rem 1rem;
            background: radial-gradient(circle at center, rgba(0, 242, 255, 0.1) 0%, transparent 70%);
        }

        .page-title {
            font-size: 3rem;
            background: linear-gradient(to right, #fff, #00f2ff);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            font-weight: 800;
            margin-bottom: 10px;
        }

        .page-subtitle { color: #94a3b8; font-size: 1.1rem; max-width: 600px; margin: 0 auto; }

        /* KATEGORİ BAŞLIKLARI */
        .section-title {
            text-align: center; margin: 4rem 0 2rem 0;
            font-size: 1.5rem; letter-spacing: 2px; color: #fff;
            position: relative; display: inline-block; left: 50%; transform: translateX(-50%);
        }
        .section-title::after {
            content: ''; display: block; width: 60%; height: 3px; 
            background: var(--neon-purple); margin: 10px auto 0 auto; border-radius: 2px;
            box-shadow: 0 0 10px var(--neon-purple);
        }

        /* LOGO GRID YAPISI */
        .sponsor-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(160px, 1fr));
            gap: 30px;
            max-width: 1200px; margin: 0 auto; padding: 0 20px;
        }

        /* SPONSOR KARTI (GLASSMORPHISM) */
        .sponsor-card {
            background: rgba(255, 255, 255, 0.03);
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 16px;
            padding: 30px 20px;
            display: flex; flex-direction: column; align-items: center; justify-content: center;
            transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
            text-decoration: none;
            position: relative;
            overflow: hidden;
        }

        /* LOGO RESMİ */
        .sponsor-card img {
            max-width: 100%;
            max-height: 70px;
            object-fit: contain;
            filter: grayscale(100%) opacity(0.6); /* Başlangıçta gri ve soluk */
            transition: 0.4s;
        }

        .sponsor-name {
            margin-top: 15px;
            color: #64748b;
            font-size: 0.9rem;
            font-weight: 500;
            transition: 0.3s;
        }

        /* HOVER EFEKTLERİ (MOUSE ÜSTÜNE GELİNCE) */
        .sponsor-card:hover {
            transform: translateY(-5px);
            background: rgba(255, 255, 255, 0.08);
            border-color: var(--neon-blue);
            box-shadow: 0 0 20px rgba(0, 242, 255, 0.15);
        }

        .sponsor-card:hover img {
            filter: grayscale(0%) opacity(1); /* Renkli ve net */
            transform: scale(1.1);
        }

        .sponsor-card:hover .sponsor-name {
            color: #fff;
            text-shadow: 0 0 10px var(--neon-blue);
        }

        /* MAIN SPONSOR ÖZEL AYARI */
        .main-sponsor-grid {
            display: flex; justify-content: center; margin-bottom: 2rem;
        }
        .main-sponsor-grid .sponsor-card {
            width: 300px; padding: 40px;
            background: linear-gradient(145deg, rgba(255,255,255,0.05), rgba(0,242,255,0.05));
            border: 1px solid var(--neon-blue);
        }
        .main-sponsor-grid img { max-height: 100px; filter: grayscale(0%) opacity(1); } /* Main hep renkli */

    </style>
</head>
